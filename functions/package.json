{
  "name": "functions",
  "version": "0.1.0",
  "type": "commonjs",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "build": "npm run clear && npm run lint && npm run compile",
    "build:watch": "npm run clear && npm run lint && npm run compile:watch",
    "clear": "rm -rf ./lib",
    "compile": "tsc -p ./tsconfig.json --pretty",
    "compile:watch": "tsc --watch -p ./tsconfig.json --pretty",
    "eslint": "eslint",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@fabricelements/shared-helpers": "file:..",
    "@google-cloud/functions-framework": "^2.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "firebase-admin": "10.0.2",
    "firebase-functions": "^3.16.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "eslint": "^8.7.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.7.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "firebase-functions-test": "^0.3.3",
    "typescript": "^4.5.5"
  },
  "overrides": {},
  "private": true
}
