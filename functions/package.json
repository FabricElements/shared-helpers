{
  "name": "functions",
  "version": "1.0.0",
  "type": "module",
  "main": "./lib/index.js",
  "exports": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "build": "npm run clear && npm run lint && npm run compile",
    "build:watch": "npm run clear && npm run lint && npm run compile:watch",
    "clear": "rm -rf ./lib",
    "compile": "tsc -p ./tsconfig.json --pretty",
    "compile:watch": "tsc --watch -p ./tsconfig.json --pretty",
    "eslint": "eslint",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "engines": {
    "node": "16"
  },
  "dependencies": {
    "@fabricelements/shared-helpers": "file:..",
    "@google-cloud/functions-framework": "^3.1.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^11.5.0",
    "firebase-functions": "^4.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^39.7.5",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-promise": "^6.1.1",
    "firebase-functions-test": "^3.0.0",
    "typescript": "^4.9.5"
  },
  "overrides": {},
  "private": true
